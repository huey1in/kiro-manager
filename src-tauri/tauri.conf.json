{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Kiro Manager",
  "version": "1.0.1",
  "identifier": "com.kiro.manager",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "账号管理器",
        "width": 800,
        "height": 600,
        "minWidth": 800,
        "minHeight": 600,
        "maxWidth": 800,
        "maxHeight": 600,
        "resizable": false,
        "fullscreen": false,
        "center": false,
        "visible": false,
        "decorations": false
      }
    ],
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": true,
        "scope": ["**"]
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
